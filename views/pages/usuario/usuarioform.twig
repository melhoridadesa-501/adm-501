{% extends 'index.twig' %}

{% block content %}

    <form>
        <div class="form-group">
            <label for="nome">Nome</label>
            <input type="text" class="form-control" id="nome" aria-describedby="Nome" placeholder="Enter name">
        </div>
        <div class="form-group">
            <label for="email">Email</label>
            <input type="text" class="form-control" id="cpf" aria-describedby="Cpf" placeholder="Enter CPF">
        </div>
        <div class="form-group">
            <label for="dtNascimento">Data de Nascimento</label>
            <input type="date" class="form-control" id="dtNascimento" aria-describedby="dtNascimento" placeholder="Enter Data de Nascimento">
        </div>

        <button type="submit" class="btn btn-primary">Submit</button>
    </form>

{% endblock %}